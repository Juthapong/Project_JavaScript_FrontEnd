<!-- books.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title>players</title>
  <link rel="stylesheet" href="../css/style.css" />
</head>
<body>
  <h1>players</h1>
  <div class="link-center">
    <a href="/create">Add player</a>
  </div>
  <table class="football-table">
    <tr>
      <th>nameplayer</th>
      
      <th>Actions</th>
    </tr>
    <% for(var i=0; i < players.length; i++) { %>
    <tr>
      <td><%= players[i].nameplayer %></td>
      
      <td>
        <!-- <a href="/player/<%= players[i].id %>">View</a> -->
        <a href="/update/<%= players[i].id %>">Edit</a>
        <a href="/delete/<%= players[i].id %>">Delete</a>
      </td>
    </tr>
    <% } %>
  </table>
  
</body>
</html>